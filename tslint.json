{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "no-arg": true,
    "no-bitwise": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning"
    },
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-any": true,
    "no-empty": true
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
